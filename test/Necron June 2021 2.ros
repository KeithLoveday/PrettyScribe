              <characteristics>
                <characteristic typeId="21befb24-fc85-4f52-a745-64b2e48f8228" name="Description">Each time this model makes an attack, re-roll a hit roll of 1.</characteristic>
              </characteristics>
              <modifiers />
              <modifierGroups />
            </profile>
